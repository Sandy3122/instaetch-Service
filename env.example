# Server Configuration
PORT=3000
NODE_ENV=development

# Security
JWT_SECRET=your-jwt-secret-here
API_KEY_SECRET=your-api-key-secret-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Instagram Scraper Configuration
INSTAGRAM_USER_AGENT=Mozilla/5.0 (Linux; Android 6.0; Nexus 5 Build/MRA58N) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/131.0.0.0 Mobile Safari/537.36
INSTAGRAM_TIMEOUT=60000

# Proxy Configuration
USE_PROXY=true
PROXY_RETRIES=3

# Cache Configuration
CACHE_TTL=3600
CACHE_CHECK_PERIOD=600

# Logging
LOG_LEVEL=info
LOG_MEDIA=false
LOG_SESSION=false
LOG_INTERCEPT=false

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:8080

# ScraperAPI (if using)
SCRAPER_API_KEY=your-scraper-api-key-here
SCRAPER_API_URL=https://api.scraperapi.com 