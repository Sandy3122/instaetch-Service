# Server Configuration
PORT=3000
NODE_ENV=development

# Security
JWT_SECRET=your-super-secret-jwt-key-here
API_KEY_SECRET=your-api-key-secret-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Instagram Scraping
INSTAGRAM_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36
INSTAGRAM_TIMEOUT=120000
INSTAGRAM_BASE_URL=https://www.instagram.com
INSTAGRAM_API_URL=https://www.instagram.com
# Get these values from your Instagram web session (inspect cookies after logging in)
INSTAGRAM_SESSION_ID=your_session_id_here
INSTAGRAM_CSRF_TOKEN=your_csrf_token_here

# Cache Configuration
CACHE_TTL=3600
CACHE_CHECK_PERIOD=600

# Logging
LOG_LEVEL=info

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://yourdomain.com

# ScraperAPI (if using)
SCRAPER_API_KEY=your-scraper-api-key-here
SCRAPER_API_URL=https://api.scraperapi.com 